{
    "name": "hybridly/demo",
    "type": "project",
    "description": "A demo application for the Hybridly project.",
    "keywords": [
        "hybridly",
        "demo"
    ],
    "repositories": [
        {
            "type": "path",
            "url": "/Users/enzoinnocenzi/Code/projects/hybridly/hybridly/packages/laravel"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.8.1",
        "hybridly/laravel": "0.x-dev",
        "laravel/framework": "^10.45.1",
        "laravel/sanctum": "^3.3.3",
        "laravel/tinker": "^2.9.0",
        "league/flysystem-path-prefixing": "^3.23.1",
        "lorisleiva/laravel-actions": "^2.7.3",
        "spatie/laravel-data": "^4.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.15.1",
        "fakerphp/faker": "^1.23.1",
        "friendsofphp/php-cs-fixer": "^3.50.0",
        "itsgoingd/clockwork": "^5.2.0",
        "laravel/sail": "^1.28.0",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^7.10.0",
        "pestphp/pest": "^2.34.0",
        "pestphp/pest-plugin-laravel": "^2.3",
        "spatie/laravel-ignition": "^2.4.2",
        "spatie/laravel-ray": "^1.35.1",
        "spatie/laravel-typescript-transformer": "^2.4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Foo\\": "src/Foo/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "style": [
            "php-cs-fixer fix --allow-risky=yes"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@autocomplete"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "autocomplete": [
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models -M"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
