{
	"repositories": [{
		"type": "path",
		"url": "/Users/enzoinnocenzi/Code/projects/hybridly/hybridly/packages/laravel"
	}],
	"name": "hybridly/demo",
	"type": "project",
	"description": "A demo application for the Hybridly project.",
	"keywords": [
		"hybridly",
		"demo"
	],
	"license": "MIT",
	"require": {
		"php": "^8.0.2",
		"guzzlehttp/guzzle": "^7.5",
		"hybridly/laravel": "*",
		"laravel/framework": "^9.38",
		"laravel/sanctum": "^3.0.1",
		"laravel/tinker": "^2.7.2",
		"league/flysystem-path-prefixing": "^3.10",
		"lorisleiva/laravel-actions": "^2.4.1",
		"pestphp/pest": "^1.22.1",
		"pestphp/pest-plugin-laravel": "^1.3",
		"spatie/laravel-data": "^2.0.13"
	},
	"require-dev": {
		"barryvdh/laravel-ide-helper": "^2.12.3",
		"fakerphp/faker": "^1.20",
		"friendsofphp/php-cs-fixer": "^3.13",
		"itsgoingd/clockwork": "^5.1.11",
		"laravel/sail": "^1.16.2",
		"mockery/mockery": "^1.5.1",
		"nunomaduro/collision": "^6.3.1",
		"phpunit/phpunit": "^9.5.26",
		"spatie/laravel-ignition": "^1.6.1",
		"spatie/laravel-ray": "^1.31",
		"spatie/laravel-typescript-transformer": "^2.1.5"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"style": [
			"php-cs-fixer fix --allow-risky=yes"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi",
			"@autocomplete"
		],
		"post-update-cmd": [
			"@php artisan vendor:publish --tag=laravel-assets --ansi --force"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		],
		"autocomplete": [
			"@php artisan ide-helper:eloquent",
			"@php artisan ide-helper:generate",
			"@php artisan ide-helper:meta",
			"@php artisan ide-helper:models -M"
		]
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true,
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
