{
	"repositories": [
		{
			"type": "path",
			"url": "/Users/enzoinnocenzi/Code/projects/hybridly/hybridly"
		}
	],
	"name": "hybridly/demo",
	"type": "project",
	"description": "A demo application for the Hybridly project.",
	"keywords": [
		"hybridly",
		"demo"
	],
	"license": "MIT",
	"require": {
		"php": "^8.0.2",
		"guzzlehttp/guzzle": "^7.5",
		"innocenzi/laravel-vite": "^0.2.2",
		"laravel/framework": "^9.30.1",
		"laravel/sanctum": "^2.15.1",
		"laravel/tinker": "^2.7.2",
		"lorisleiva/laravel-actions": "^2.4",
		"hybridly/laravel": "@dev",
		"spatie/laravel-data": "^2.0.10"
	},
	"require-dev": {
		"barryvdh/laravel-ide-helper": "^2.12.3",
		"fakerphp/faker": "^1.20",
		"friendsofphp/php-cs-fixer": "^3.11",
		"laravel/sail": "^1.16",
		"mockery/mockery": "^1.5.1",
		"nunomaduro/collision": "^6.3",
		"phpunit/phpunit": "^9.5.24",
		"spatie/laravel-ignition": "^1.5",
		"spatie/laravel-ray": "^1.30",
		"spatie/laravel-typescript-transformer": "^2.1.5"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"style": [
			"php-cs-fixer fix --allow-risky=yes"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi",
			"@autocomplete"
		],
		"post-update-cmd": [
			"@php artisan vendor:publish --tag=laravel-assets --ansi --force"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		],
		"autocomplete": [
				"@php artisan ide-helper:eloquent",
				"@php artisan ide-helper:generate",
				"@php artisan ide-helper:meta",
				"@php artisan ide-helper:models -M"
		]
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
