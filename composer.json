{
	"name": "hybridly/demo",
	"type": "project",
	"description": "A demo application for the Hybridly project.",
	"keywords": [
		"hybridly",
		"demo"
	],
	"license": "MIT",
	"require": {
		"php": "^8.0.2",
		"guzzlehttp/guzzle": "^7.5",
		"hybridly/laravel": "^0.1.0@alpha",
		"laravel/framework": "^10.4.1",
		"laravel/sanctum": "^3.2.1",
		"laravel/tinker": "^2.8.1",
		"league/flysystem-path-prefixing": "^3.10.4",
		"lorisleiva/laravel-actions": "^2.5.1",
		"spatie/laravel-data": "^3.2.1"
	},
	"require-dev": {
		"barryvdh/laravel-ide-helper": "^2.13.0",
		"fakerphp/faker": "^1.21",
		"friendsofphp/php-cs-fixer": "^3.15.1",
		"itsgoingd/clockwork": "^5.1.12",
		"laravel/sail": "^1.21.2",
		"mockery/mockery": "^1.5.1",
		"nunomaduro/collision": "^7.3.3",
		"pestphp/pest": "^2.2.3",
		"pestphp/pest-plugin-laravel": "^2.0",
		"spatie/laravel-ignition": "^2.0.0",
		"spatie/laravel-ray": "^1.32.4",
		"spatie/laravel-typescript-transformer": "^2.2.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"style": [
			"php-cs-fixer fix --allow-risky=yes"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi",
			"@autocomplete"
		],
		"post-update-cmd": [
			"@php artisan vendor:publish --tag=laravel-assets --ansi --force"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		],
		"autocomplete": [
			"@php artisan ide-helper:eloquent",
			"@php artisan ide-helper:generate",
			"@php artisan ide-helper:meta",
			"@php artisan ide-helper:models -M"
		]
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true,
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
